{
  "name": "@worldstreet/vivid-voice",
  "version": "0.1.0",
  "description": "Vivid - AI Voice Agent SDK for Next.js by Worldstreet",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./functions": {
      "types": "./dist/functions.d.ts",
      "import": "./dist/functions.mjs",
      "require": "./dist/functions.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "next": "^14.0.0 || ^15.0.0",
    "@clerk/nextjs": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "@clerk/nextjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "voice",
    "ai",
    "agent",
    "nextjs",
    "openai",
    "realtime",
    "worldstreet",
    "vivid"
  ],
  "author": "Worldstreet",
  "license": "MIT"
}
